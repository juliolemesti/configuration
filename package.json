{
  "name": "@ramonornela/configuration",
  "version": "0.0.9",
  "description": "Gerenciamento de configuração do projeto em arquivo json, database ou objeto estático",
  "main": "dist/umd/index.js",
  "typings": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "prepush": "npm run lint",
    "commit": "git-cz",
    "lint": "tslint -c ./tslint.json --project ./tsconfig-esm.json --force",
    "package:json": "node ./scripts/package",
    "build:umd": "ngc",
    "build:esm": "ngc -p tsconfig-esm.json",
    "build": "npm run clean && npm run build:umd && npm run build:esm",
    "build:lint": "npm run lint && npm run build",
    "publishPackage": "npm run build:lint && npm run package:json --prepare && npm publish --access public && npm run package:json --restore"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@angular/common": "^2.0.1",
    "@angular/core": "^2.0.1",
    "@angular/http": "^2.0.1",
    "@angular/platform-browser": "^2.0.1"
  },
  "devDependencies": {
    "@angular/compiler": "~2.1.0",
    "@angular/compiler-cli": "~2.1.0",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.3",
    "tslint": "3.15.1",
    "tslint-eslint-rules": "1.5.0",
    "tslint-ionic-rules": "0.0.8",
    "rimraf": "^2.5.4",
    "@types/jasmine": "2.2.33",
    "jasmine": "2.5.2",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.11.9"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "@angular/http": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramonornela/configuration.git"
  },
  "keywords": [
    "ionic",
    "angular",
    "url",
    "http",
    "headers",
    "get",
    "post"
  ],
  "author": "Ramon Ornelas <ramon.ornela@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ramonornela/configuration/issues"
  },
  "homepage": "https://github.com/ramonornela/configuration"
}
